 productKeys = Object.keys(savedCart);\r\n        const cartProducts = productKeys.map(existingKey =>{\r\n            const product = fakeData.find(pd => pd.key === existingKey)\r\n            product.quantity = savedCart[existingKey];\r\n           \r\n            return product;\r\n        })\r\n        setCart(cartProducts);\r\n    },[])\r\n\r\n    const handleAddProduct = (product) =>{\r\n        const toBeAddedKey = product.key;\r\n        const sameProduct = cart.find(pd => pd.key === toBeAddedKey);\r\n        let count = 1;\r\n        let newCart;\r\n        if(sameProduct){\r\n            count = sameProduct.quantity + 1;\r\n            sameProduct.quantity = count;\r\n            const others = cart.filter(pfd => pfd.key !== toBeAddedKey);\r\n            newCart = [...others, sameProduct];\r\n        }\r\n        else{\r\n            product.quantity = 1;\r\n            newCart = [...cart, product];\r\n        }\r\n\r\n        setCart(newCart);\r\n        addToDatabaseCart(product.key, count);\r\n    }\r\n \r\n    return (\r\n        <div className=\"twin-container\">\r\n            <div className=\"product-container\">\r\n                {\r\n                    products.map(product => <Product\r\n                        showAddToCart= {true}\r\n                        key={product.key}\r\n                        handleAddProduct={handleAddProduct}\r\n                        product={product}\r\n                        ></Product>)\r\n                }\r\n            </div>\r\n            <div  className=\"cart-container\">\r\n                <Cart cart={cart}>\r\n                    <Link to=\"/review\"> <button className=\"btn\">Review Your Order</button></Link>\r\n                </Cart>\r\n                \r\n            </div>\r\n           \r\n        </div>\r\n    );\r\n};\r\n\r\nexport default Shop;","C:\\Web-development\\ema-john\\src\\fakeData\\index.js",[],"C:\\Web-development\\ema-john\\src\\fakeData\\android.js",[],"C:\\Web-development\\ema-jo